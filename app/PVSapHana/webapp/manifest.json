{
	"_version": "1.12.0",
	"sap.app": {
		"id": "PVSapHana",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "${version}"
		},
		"sourceTemplate": {
			"id": "generator-ui5-app",
			"version": "1.1.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/odata/v4/product/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "PVSapHana.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.136.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"handleValidation": true,
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": false,
					"bundleName": "PVSapHana.i18n.i18n",
					"supportedLocales": [
						"en",
						"de"
					],
					"fallbackLocale": "en",
					"async": true
				}
			},
			"": {
				"type": "sap.ui.model.odata.v4.ODataModel",
				"dataSource": "mainService",
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "PVSapHana.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main"
				},
				{
					"pattern": "",
					"name": "inicio",
					"target": "inicio"
				},
				{
					"pattern": "ventas",
					"name": "ventas",
					"target": "ventas"
				},
				{
					"pattern": "negocios",
					"name": "negocios",
					"target": "negocios"
				},
				{
					"pattern": "productos",
					"name": "productos",
					"target": "productos"
				},
				{
					"pattern": "empleados",
					"name": "empleados",
					"target": "empleados"
				},
				{
					"pattern": "configuracion",
					"name": "configuracion",
					"target": "configuracion"
				}
			],
			"targets": {
				"main": {
					"viewId": "main",
					"viewName": "Main"
				},
				"ventas": {
					"viewName": "Ventas",
					"viewId": "ventas",
					"viewLevel": 2
				},
				"negocios": {
					"viewName": "Negocios",
					"viewId": "negocios",
					"viewLevel": 2
				},
				"productos": {
					"viewName": "Productos",
					"viewId": "productos",
					"viewLevel": 2
				},
				"empleados": {
					"viewName": "Empleados",
					"viewId": "empleados",
					"viewLevel": 2
				},
				"configuracion": {
					"viewName": "Configuracion",
					"viewId": "configuracion",
					"viewLevel": 3
				}
			}
		}
	}
}